{
	"name": "effect-demo",
	"private": true,
	"workspaces": {
		"packages": [
			"packages/*"
		],
		"catalog": {
			"@biomejs/biome": "2.3.2",
			"@prisma/client": "7.0.1",
			"@prisma/adapter-pg": "7.0.1",
			"@lynx-js/preact-devtools": "5.0.1-cf9aef5",
			"@lynx-js/qrcode-rsbuild-plugin": "0.4.3",
			"@lynx-js/react-rsbuild-plugin": "0.11.4",
			"@lynx-js/rspeedy": "0.12.0",
			"@lynx-js/types": "3.4.11",
			"@rsbuild/plugin-type-check": "1.2.4",
			"@lynx-js/react": "0.114.5",
			"@types/react": "18.3.25",
			"prisma": "7.0.1",
			"hono": "4.10.6",
			"zod": "4.1.13",
			"turbo": "2.6.1",
			"typescript": "5.9.3",
			"bun-types": "1.3.3"
		}
	},
	"scripts": {
		"auth:docker": "rm -rf ~/.docker && echo $(gh auth token) | docker login ghcr.io -u $(gh api user | jq -r '.login') --password-stdin",
		"build": "turbo build",
		"build:docker": "turbo build:docker",
		"clean": "sudo find . -name \"node_modules\" -type d -prune -exec rm -rf {} +",
		"db:migrate": "bun run --cwd ./packages/database migrate",
		"db:reset": "bun run --cwd ./packages/database reset",
		"dev": "turbo dev",
		"format": "biome format --write .",
		"lint": "turbo lint"
	},
	"devDependencies": {
		"@effect-demo/tsconfig": "workspace:*",
		"@biomejs/biome": "catalog:",
		"turbo": "catalog:",
		"bun-types": "catalog:"
	},
	"peerDependencies": {
		"typescript": "catalog:"
	},
	"packageManager": "bun@1.3.3"
}

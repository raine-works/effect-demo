{
	"name": "effect-demo",
	"private": true,
	"workspaces": {
		"packages": [
			"packages/*"
		],
		"catalog": {
			"@biomejs/biome": "2.3.2",
			"@effect/language-service": "0.56.0",
			"@effect/platform": "0.93.2",
			"@effect/platform-bun": "0.83.0",
			"@effect/rpc": "0.72.1",
			"@effect-rx/rx-react": "0.42.4",
			"@prisma/client": "7.0.1",
			"@prisma/adapter-pg": "7.0.1",
			"effect": "3.19.6",
			"prisma": "7.0.1",
			"turbo": "2.5.8",
			"typescript": "5.9.3",
			"bun-types": "1.3.3"
		}
	},
	"scripts": {
		"auth:docker": "rm -rf ~/.docker && echo $(gh auth token) | docker login ghcr.io -u $(gh api user | jq -r '.login') --password-stdin",
		"build": "turbo build",
		"build:docker": "turbo build:docker",
		"clean": "sudo find . -name \"node_modules\" -type d -prune -exec rm -rf {} +",
		"db:migrate": "bun run --cwd ./packages/database migrate",
		"db:reset": "bun run --cwd ./packages/database reset",
		"dev": "turbo dev",
		"format": "biome format --write .",
		"lint": "turbo lint"
	},
	"devDependencies": {
		"@effect-demo/tsconfig": "workspace:*",
		"@effect/language-service": "catalog:",
		"@biomejs/biome": "catalog:",
		"turbo": "catalog:",
		"bun-types": "catalog:"
	},
	"peerDependencies": {
		"typescript": "catalog:"
	},
	"packageManager": "bun@1.3.3"
}

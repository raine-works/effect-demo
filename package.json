{
	"name": "effect-demo",
	"private": true,
	"workspaces": {
		"packages": [
			"packages/*"
		],
		"catalog": {
			"@biomejs/biome": "2.3.2",
			"@effect/platform": "0.93.2",
			"@effect/platform-bun": "0.83.0",
			"@effect/rpc": "0.72.1",
			"@effect/sql": "0.48.0",
			"@effect-rx/rx-react": "0.42.4",
			"effect": "3.19.3",
			"turbo": "2.5.8",
			"typescript": "5.9.3",
			"bun-types": "1.3.2"
		}
	},
	"scripts": {
		"auth:docker": "rm -rf ~/.docker && echo $(gh auth token) | docker login ghcr.io -u $(gh api user | jq -r '.login') --password-stdin",
		"build": "turbo build",
		"build:docker": "turbo build:docker",
		"dev": "turbo dev",
		"format": "biome format --write .",
		"lint": "turbo lint"
	},
	"devDependencies": {
		"@effect-demo/tsconfig": "workspace:*",
		"@biomejs/biome": "catalog:",
		"turbo": "catalog:",
		"bun-types": "catalog:"
	},
	"peerDependencies": {
		"typescript": "catalog:"
	},
	"packageManager": "bun@1.3.2"
}

{
	"name": "Effect Demo Workspace",
	"dockerComposeFile": "docker-compose.yaml",
	"service": "workspace",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"features": {
		"ghcr.io/devcontainers-extra/features/turborepo-npm:1": {
			"version": "2.6.1"
		},
		"ghcr.io/raine-works/devcontainer-features/bun:1": { "version": "1.3.3" },
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {},
		"ghcr.io/devcontainers/features/github-cli:1": {}
	},
	"mounts": [
		"source=${localEnv:HOME}/.config/gh,target=/home/vscode/.config/gh,type=bind"
	],
	"containerEnv": {
		"HOST_IP": "${localEnv:HOST_IP}",
		"NODE_ENV": "development",
		"TZ": "UTC"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"bradlc.vscode-tailwindcss",
				"yoavbls.pretty-ts-errors",
				"expo.vscode-expo-tools",
				"mhutchie.git-graph",
				"waderyan.gitblame",
				"oven.bun-vscode",
				"biomejs.biome",
				"Prisma.prisma"
			]
		}
	}
}

generator client {
  provider = "prisma-client"
  output   = "../src/generated"
  engineType = "client"
  runtime = "bun"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  email     String   @unique
  name      String?
}